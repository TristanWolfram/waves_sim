<?xml version="1.0"?>
    <scenario>
    
        <static name="Fisherboat" type="model">
            <physical>
                <mesh filename="boats/fisher_boat_phys.obj" scale="1.0"/>
                <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            </physical>
            <visual>
                <mesh filename="boats/fisher_boat.obj" scale="1.0"/>
                <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            </visual>
            <material name="Boat"/>
            <look name="Fisherboat" uv_mode="0"/>
            <world_transform xyz="100.0 155.0 0.5" rpy="-1.57 0.0 0.0"/>
            <!--Front-->
            <sensor name="DcamF" rate="10.0" type="depthcamera">
                <specs resolution_x="256" resolution_y="128" horizontal_fov="90.0" depth_min="0.2" depth_max="100.0" />
                <origin xyz="-1.0 3.0 0.0" rpy="0.0 1.57 3.14" />
                <ros_publisher topic="/sim_camF_depth" />
            </sensor>
            <!--Right-->
            <sensor name="DcamR" rate="10.0" type="depthcamera">
                <specs resolution_x="256" resolution_y="128" horizontal_fov="90.0" depth_min="0.2" depth_max="100.0" />
                <origin xyz="-1.0 3.0 0.0" rpy="0.0 3.14 3.14" />
                <ros_publisher topic="/sim_camR_depth" />
            </sensor>
            <!--Left-->
            <sensor name="DcamL" rate="10.0" type="depthcamera">
                <specs resolution_x="256" resolution_y="128" horizontal_fov="90.0" depth_min="0.2" depth_max="100.0" />
                <origin xyz="-1.0 3.0 0.0" rpy="0.0 0.0 3.14" />
                <ros_publisher topic="/sim_camL_depth" />
            </sensor>
            <!--Back-->
            <sensor name="DcamB" rate="10.0" type="depthcamera">
                <specs resolution_x="256" resolution_y="128" horizontal_fov="90.0" depth_min="0.2" depth_max="100.0" />
                <origin xyz="-1.0 3.0 0.0" rpy="0.0 4.71 3.14" />
                <ros_publisher topic="/sim_camB_depth" />
            </sensor>
        </static>

        <static name="SensorPlatform" type="box">
            <dimensions xyz="0.01 0.01 0.01"/>
            <material name="Neutral"/>
            <look name="LightGray"/>
            <world_transform xyz="150.0 90.0 -2.0" rpy="-1.57 0.0 3.14"/>
        </static>

        <animated name="MovingBoat0" type="model" collition="true">
            <physical>
                <mesh filename="boats/res_boat_cube_phys.obj" scale="1.0"/>
                <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            </physical>
            <visual>
                <mesh filename="boats/res_boat.obj" scale="1.0"/>
                <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            </visual>
            <material name="Boat"/>
            <look name="LightGray"/>
            <trajectory type="spline" playback="repeat">
                <keypoint time="0.0" xyz="205.0 150.0 0.5" rpy="-1.57 0.0 1.57"/>
                <keypoint time="30.0" xyz="205.0 10.0 0.5" rpy="-1.57 0.0 1.57"/>
            </trajectory>
        </animated>

    </scenario>
